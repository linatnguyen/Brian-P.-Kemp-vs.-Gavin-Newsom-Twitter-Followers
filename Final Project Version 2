   "cell_type": "code",
   "execution_count": 11,
   "id": "3db949c3",
   "metadata": {},
   "outputs": [],
   "source": [
    "def clean_text(txt):\n",
    "    if type(txt) != str:\n",
    "        return \"\"\n",
    "    txt = txt.lower()\n",
    "    txt = re.sub('@[A-Za-z0-9_]+', '', txt)\n",
    "    txt = re.sub('#[A-Za-z0-9_]+', '', txt)\n",
    "    txt = re.sub(r'http\\S+', '', txt)\n",
    "    txt = txt.replace('\\n', ' ')\n",
    "    txt = re.sub('\\s+', ' ', txt)\n",
    "    txt = re.sub('[^a-z\\s]','', txt)\n",
    "    return txt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "250630c6",
