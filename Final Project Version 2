    "vec = TfidfVectorizer(stop_words=mystopword)\n",
    "vec.fit(train['clean_text'])\n",
    "X_train = vec.transform(train['clean_text'])\n",
    "y_train = train['id']\n",
    "X_test = vec.transform(test['clean_text'])\n",
    "y_test = test['id']\n",
    "X_train.shape"
   ]
